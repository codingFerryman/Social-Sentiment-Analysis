{
  "num_train_epochs": 3,
  "per_device_train_batch_size": 800,
  "per_device_eval_batch_size": 800,
  "adafactor": true,
  "warmup_steps": 100,
  "weight_decay": 0.0001,
  "learning_rate": 5e-5,
  "evaluation_strategy": "epoch",
